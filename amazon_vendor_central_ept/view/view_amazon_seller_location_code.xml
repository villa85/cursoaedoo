<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_tree_amazon_seller_location_code" model="ir.ui.view">
            <field name="name">Amazon seller Location Code Tree view</field>
            <field name="model">amazon.seller.location.code</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="gln_number"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="view_form_amazon_seller_location_code" model="ir.ui.view">
            <field name="name">Amazon Seller Location Code form view</field>
            <field name="model">amazon.seller.location.code</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <div class="oe_title" style="width: 390px;">
                            <label class="oe_edit_only" for="name" string="Seller"/>
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="gln_number"/>
                                <field name="description"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</openerp>